{
  "project_name": "Base Project",
  "project_slug": "{{ cookiecutter.project_name|lower|replace(' ', '-') }}",
  "test_auth_realm": "{{cookiecutter.project_slug}}-realm",
  "enable_adapter": "n",
  "enable_cache": "n",
  "enable_ui": "n",
  "enable_proxy": "n",
  "enable_spot": "y",
  "docker_image_prefix": "d4",
  "docker_image_spot": "{{cookiecutter.docker_image_prefix}}_{{cookiecutter.project_slug}}_spot",
  "d4service_oauth2_uri": "http://keycloak:8080",
  "d4service_oauth2_hostname": "keycloak",
  "d4service_oauth2_port": 8080,
  "d4service_oauth2_client_id": "d4-client",
  "d4service_oauth2_client_secret": "1f02bb76-eb58-40e2-928d-13f5c35794c1",
  "d4service_oauth2_well_known_endpoint": null,
  "d4service_oauth2_issuer": "{{cookiecutter.d4service_oauth2_uri}}/auth/realms/{{cookiecutter.test_auth_realm}}",
  "d4service_oauth2_authorization_endpoint": "{{cookiecutter.d4service_oauth2_uri}}/auth/realms/{{cookiecutter.test_auth_realm}}/protocol/openid-connect/auth",
  "d4service_oauth2_token_endpoint": "{{cookiecutter.d4service_oauth2_uri}}/auth/realms/{{cookiecutter.test_auth_realm}}/protocol/openid-connect/token",
  "d4service_oauth2_jwks_uri": "{{cookiecutter.d4service_oauth2_uri}}/auth/realms/{{cookiecutter.test_auth_realm}}/protocol/openid-connect/certs",
  "d4service_oauth2_userinfo_endpoint": "{{cookiecutter.d4service_oauth2_uri}}/auth/realms/{{cookiecutter.test_auth_realm}}/protocol/openid-connect/userinfo",
  "d4service_opa_uri": "http://opa:8181",
  "d4service_opa_hostname": "opa",
  "d4service_opa_port": 8181,
  "d4service_spot_uri": "http://spot:9030",
  "d4service_spot_hostname": "spot",
  "d4service_spot_port": 9030
}
